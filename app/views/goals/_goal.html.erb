<div id="<%= dom_id goal %>">
  <p>
    <strong>Title:</strong>
    <%= goal.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= goal.description %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= goal.start_date %>
  </p>

  <p>
    <strong>Finish date:</strong>
    <%= goal.finish_date %>
  </p>

  <p>
    <strong>Attrib:</strong>
    <%= goal.attrib %>
  </p>

  <p>
    <strong>Team:</strong>
    <%= goal.team %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= goal.status %>
  </p>

</div>
